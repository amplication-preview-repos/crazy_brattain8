datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model QrCode {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
  url       String?
}

model Wallet {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  publicKey String?
  updatedAt DateTime @updatedAt
}
